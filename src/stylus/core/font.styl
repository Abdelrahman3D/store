@font-face
  font-weight: 500
  font-style: normal
  font-family: 'Lato'
  src: local('Lato'), url('/src/assets/fonts/Lato-Regular.ttf') format('truetype')
  font-display: swap

@font-face
  font-weight: 700
  font-style: normal
  font-family: 'Lato'
  src: local('Lato Bold'), url('/src/assets/fonts/Lato.ttf') format('truetype')
  font-display: swap

body
  font-family: 'Lato', -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Helvetica Neue', Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol'
